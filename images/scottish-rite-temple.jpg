<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <style>
      .cls-1 { fill: #f5f5f5; }
      .cls-2 { fill: #1a237e; }
      .cls-3 { fill: #ffd700; }
      .cls-4 { fill: #8b0000; }
      .cls-5 { fill: #666666; }
    </style>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="400" class="cls-1"/>
  
  <!-- Temple building -->
  <g transform="translate(200,150)">
    <!-- Main temple structure -->
    <rect x="0" y="100" width="400" height="200" class="cls-2"/>
    
    <!-- Temple front with columns -->
    <rect x="50" y="120" width="300" height="180" fill="#2c3e9e"/>
    
    <!-- Columns -->
    <rect x="80" y="150" width="20" height="150" class="cls-3"/>
    <rect x="130" y="150" width="20" height="150" class="cls-3"/>
    <rect x="180" y="150" width="20" height="150" class="cls-3"/>
    <rect x="230" y="150" width="20" height="150" class="cls-3"/>
    <rect x="280" y="150" width="20" height="150" class="cls-3"/>
    
    <!-- Column capitals -->
    <rect x="75" y="145" width="30" height="8" class="cls-3"/>
    <rect x="125" y="145" width="30" height="8" class="cls-3"/>
    <rect x="175" y="145" width="30" height="8" class="cls-3"/>
    <rect x="225" y="145" width="30" height="8" class="cls-3"/>
    <rect x="275" y="145" width="30" height="8" class="cls-3"/>
    
    <!-- Pediment -->
    <polygon points="50,120 200,60 350,120" class="cls-3"/>
    
    <!-- Scottish Rite symbol on pediment -->
    <circle cx="200" cy="90" r="15" class="cls-2"/>
    <text x="200" y="97" text-anchor="middle" font-family="serif" font-size="12" fill="white">32Â°</text>
    
    <!-- Temple doors -->
    <rect x="170" y="230" width="60" height="70" class="cls-4"/>
    <rect x="175" y="235" width="50" height="60" class="cls-5"/>
    
    <!-- Temple steps -->
    <rect x="30" y="290" width="340" height="10" class="cls-5"/>
    <rect x="40" y="300" width="320" height="10" class="cls-5"/>
    <rect x="50" y="310" width="300" height="10" class="cls-5"/>
  </g>
  
  <!-- Landscaping -->
  <g transform="translate(0,400)">
    <!-- Ground -->
    <rect x="0" y="0" width="800" height="200" fill="#4a5d23"/>
    
    <!-- Trees -->
    <g transform="translate(100,50)">
      <rect x="0" y="20" width="8" height="30" fill="#4a5d23"/>
      <circle cx="4" cy="20" r="15" fill="#2d3d14"/>
    </g>
    
    <g transform="translate(650,40)">
      <rect x="0" y="25" width="10" height="35" fill="#4a5d23"/>
      <circle cx="5" cy="25" r="18" fill="#2d3d14"/>
    </g>
    
    <!-- Pathway -->
    <rect x="300" y="100" width="200" height="100" fill="#999999"/>
  </g>
  
  <!-- Title overlay -->
  <rect x="0" y="500" width="800" height="100" fill="rgba(26, 35, 126, 0.8)"/>
  <text x="400" y="540" text-anchor="middle" font-family="serif" font-size="32" fill="white">Scottish Rite Temple</text>
  <text x="400" y="570" text-anchor="middle" font-family="sans-serif" font-size="18" fill="#ffd700">Northern Masonic Jurisdiction</text>
</svg>