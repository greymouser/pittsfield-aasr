<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <style>
      .building { fill: #2c3e9e; }
      .stone { fill: #d4d4d4; }
      .gold { fill: #ffd700; }
      .shadow { fill: #1a1a1a; opacity: 0.3; }
      .window { fill: #87ceeb; }
      .door { fill: #8b4513; }
      .sky { fill: linear-gradient(to bottom, #87ceeb 0%, #e0f6ff 100%); }
      .ground { fill: #2d5016; }
      .path { fill: #999999; }
    </style>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="400" class="sky"/>
  
  <!-- Ground -->
  <rect x="0" y="400" width="800" height="200" class="ground"/>
  
  <!-- Main temple building with proper proportions -->
  <g transform="translate(150,120)">
    <!-- Building shadow -->
    <rect x="5" y="205" width="505" height="185" class="shadow"/>
    
    <!-- Main building structure -->
    <rect x="0" y="200" width="500" height="180" class="building"/>
    
    <!-- Temple front facade -->
    <rect x="50" y="220" width="400" height="160" class="stone"/>
    
    <!-- Classical pediment -->
    <polygon points="50,220 250,150 450,220" class="stone"/>
    <polygon points="60,210 250,145 440,210" class="gold"/>
    
    <!-- Columns (6 columns for proper proportions) -->
    <rect x="80" y="250" width="18" height="130" class="stone"/>
    <rect x="125" y="250" width="18" height="130" class="stone"/>
    <rect x="170" y="250" width="18" height="130" class="stone"/>
    <rect x="310" y="250" width="18" height="130" class="stone"/>
    <rect x="355" y="250" width="18" height="130" class="stone"/>
    <rect x="400" y="250" width="18" height="130" class="stone"/>
    
    <!-- Column capitals -->
    <rect x="75" y="245" width="28" height="8" class="gold"/>
    <rect x="120" y="245" width="28" height="8" class="gold"/>
    <rect x="165" y="245" width="28" height="8" class="gold"/>
    <rect x="305" y="245" width="28" height="8" class="gold"/>
    <rect x="350" y="245" width="28" height="8" class="gold"/>
    <rect x="395" y="245" width="28" height="8" class="gold"/>
    
    <!-- Column bases -->
    <rect x="75" y="380" width="28" height="5" class="gold"/>
    <rect x="120" y="380" width="28" height="5" class="gold"/>
    <rect x="165" y="380" width="28" height="5" class="gold"/>
    <rect x="305" y="380" width="28" height="5" class="gold"/>
    <rect x="350" y="380" width="28" height="5" class="gold"/>
    <rect x="395" y="380" width="28" height="5" class="gold"/>
    
    <!-- Scottish Rite symbols in pediment -->
    <g transform="translate(250,180)">
      <!-- Double-headed eagle -->
      <circle cx="0" cy="0" r="25" class="building"/>
      <text x="0" y="6" text-anchor="middle" font-family="serif" font-size="18" fill="white" font-weight="bold">32Â°</text>
    </g>
    
    <!-- Temple entrance -->
    <rect x="200" y="320" width="100" height="60" class="door"/>
    <rect x="210" y="330" width="80" height="45" class="shadow"/>
    
    <!-- Temple windows -->
    <rect x="90" y="280" width="15" height="25" class="window"/>
    <rect x="130" y="280" width="15" height="25" class="window"/>
    <rect x="355" y="280" width="15" height="25" class="window"/>
    <rect x="395" y="280" width="15" height="25" class="window"/>
    
    <!-- Side wings -->
    <rect x="-30" y="260" width="80" height="120" class="building"/>
    <rect x="450" y="260" width="80" height="120" class="building"/>
    
    <!-- Wing windows -->
    <rect x="-15" y="290" width="12" height="20" class="window"/>
    <rect x="-15" y="320" width="12" height="20" class="window"/>
    <rect x="465" y="290" width="12" height="20" class="window"/>
    <rect x="465" y="320" width="12" height="20" class="window"/>
    
    <!-- Temple steps -->
    <rect x="30" y="380" width="440" height="8" class="stone"/>
    <rect x="40" y="388" width="420" height="8" class="stone"/>
    <rect x="50" y="396" width="400" height="8" class="stone"/>
  </g>
  
  <!-- Landscaping -->
  <g transform="translate(0,450)">
    <!-- Pathway to temple -->
    <polygon points="350,150 450,150 500,50 300,50" class="path"/>
    
    <!-- Trees -->
    <g transform="translate(80,80)">
      <rect x="-3" y="20" width="6" height="40" fill="#4a5d23"/>
      <circle cx="0" cy="20" r="20" fill="#228b22"/>
    </g>
    
    <g transform="translate(680,90)">
      <rect x="-4" y="25" width="8" height="45" fill="#4a5d23"/>
      <circle cx="0" cy="25" r="25" fill="#228b22"/>
    </g>
    
    <g transform="translate(50,120)">
      <rect x="-2" y="15" width="4" height="25" fill="#4a5d23"/>
      <circle cx="0" cy="15" r="15" fill="#228b22"/>
    </g>
    
    <g transform="translate(720,110)">
      <rect x="-3" y="20" width="6" height="30" fill="#4a5d23"/>
      <circle cx="0" cy="20" r="18" fill="#228b22"/>
    </g>
  </g>
  
  <!-- Dedication plaque -->
  <g transform="translate(400,520)">
    <rect x="-100" y="0" width="200" height="60" fill="rgba(44, 62, 158, 0.9)" rx="5"/>
    <text x="0" y="20" text-anchor="middle" font-family="serif" font-size="14" fill="white" font-weight="bold">SCOTTISH RITE TEMPLE</text>
    <text x="0" y="38" text-anchor="middle" font-family="serif" font-size="12" fill="#ffd700">Northern Masonic Jurisdiction</text>
    <text x="0" y="52" text-anchor="middle" font-family="serif" font-size="10" fill="#ffd700">Ancient &amp; Accepted Scottish Rite</text>
  </g>
</svg>